    "from numpy import *\n",
    "def f(x,u):\n",
    "    return sin(x) + u\n",
    "def euler_expl(u,x,h,f):\n",
    "    return u + h * f(x,u)\n",
    "\n",
    "i = linspace(0,1,1000000000)\n",
    "h = i[1]-i[0]\n",
    "U0 = 0\n",
    "\n",
    "for elm in i:\n",
    "    U0 = euler_expl(U0,elm,h,f)\n",
    "\n",
    "print(U0)"
   ]
  }
